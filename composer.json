{
    "name": "liamja/console-boilerplate",
    "description": "Bootstrap/boilerplate for kick-starting CLI-based projects, using Symfony Console",
    "type": "project",
    "require": {
        "symfony/console": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Liam Anderson",
            "email": "liam.anderson.91@gmail.com"
        }
    ],
    "bin": [
        "bin/app"
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
